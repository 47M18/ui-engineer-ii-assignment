{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "max-len": 0,
      "react/prop-types": 0,
      "import/no-extraneous-dependencies": 0,
      "react/jsx-one-expression-per-line": 0,
      "jsx/one-expression-per-line": 0
    }
}
